<?xml version="1.0"?>
<info xmlns:xsi= "http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="https://apps.nextcloud.com/schema/apps/info.xsd">
    <id>printer2</id>
    <name>Printer2</name>
     <summary>Allows to share files rapidly for 8 hours.</summary>
     <description>
       Please check the Readme on Github on how to set up this app!

       Allows users to share files using fastlink.
    </description>
    <version>0.0.1</version>
    <licence>agpl</licence>
    <author>fattahi</author>
    <types>
      <filesystem/>
    </types>
    <documentation>
        <user>https://github.com/fatthi/nextcloud-printer/blob/master/README.md</user>
        <admin>https://github.com/fattahi/nextcloud-printer/blob/master/README.md</admin>
    </documentation>
    <category>files</category>
    <category>tools</category>
    <website>https://github.com/fattahi/nextcloud-printer/</website>
    <bugs>https://github.com/fattahi/nextcloud-printer/issues</bugs>
    <screenshot>https://github.com/fattahi/nextcloud-printer/raw/master/screenshots/printer.gif</screenshot>
         <dependencies>
        <nextcloud min-version="20" max-version="21" />
    </dependencies>
	<settings>
		<admin>OCA\Printer2\Settings\Admin\AllowedGroups</admin>
		<admin-section>OCA\Printer2\Settings\Admin\Section</admin-section>
	</settings>
</info>
